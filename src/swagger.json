{
  "swagger": "2.0",
  "info": {
    "description": "ratp api test",
    "version": "1.0.0",
    "title": "Swagger ratp test"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "tags": [
  ],
  "schemes": [
   
  ],
  "paths": {
    "/coordinate": {
      "get": {
        "summary": "get location coordinates",
        "description": "Retrieve all coordinates that fits the location name.",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "location",
            "in": "query",
            "description": "will search coordinates that contain the given location",
            "type": "string"
          },
           {
            "name": "page",
            "in": "query",
            "description": "page",
            "type": "integer"
          },
           {
            "name": "item-per-page",
            "in": "query",
            "description": "item per page",
            "type": "integer"
          },
           {
            "name": "sort-by-name",
            "in": "query",
            "description": "will search sets that contain the given title (no fuzzy for now)",
            "type": "string",
            "enum": [
                "desc",
                "asc"
              ]
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Coordinates"
              }
            }
          },
          "400": {
            "description": "Bad request"
          },
          "403": {
            "description": "Unauthorized"
          }
        },
        "security": [
          {
            "docgen_auth": [
              "read:docgenApi"
            ]
          }
        ]
      }
    }
  },

  "definitions": {
    "Coordinates": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Coordinate"
          }
        },
    "Coordinate": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "location name"
        },
        "recordid": {
          "type": "string",
           "description": "record id"
        },
       "coordinates": {
          "type": "array",
          "items": {
            "type": "integer"
          },
          "description": "coordinate data"
        },
        "description": {
          "type": "string",
           "description": "record id"
        }
      }
    }
  }
}